{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES6",
    "module": "ES6",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "include": [
    "src/**/*"
  ]
}
