{
  "name": "ypr-messenger",
  "version": "0.0.1",
  "description": "Yandex practicum learn project",
  "source": "src/index.html",
  "engines": {
    "npm": ">=8 <=9",
    "node": ">=16 <=18"
  },
  "scripts": {
    "dev": "parcel",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/*.test.ts'",
    "prebuild": "rimraf 'dist' && npm run test && npm run ts:check",
    "build": "webpack --mode=production --node-env=production",
    "preserver": "npm run build",
    "server": "node back/server.js",
    "start": "npm run server",
    "ts:check": "tsc --noEmit",
    "lint": "eslint  src/ --ext .ts",
    "lint:styles": "stylelint  src/**/*.css",
    "lint:styles:fix": "stylelint  src/**/*.css --fix",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaskes79/middle.messenger.praktikum.yandex.git"
  },
  "author": "Vasily Guzov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vaskes79/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/vaskes79/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "@webpack-cli/generators": "^3.0.1",
    "css-loader": "^6.7.3",
    "date-fns": "2.29.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "^2.7.3",
    "prettier": "2.8.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^4.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:fix",
      "git add"
    ],
    "src/**/*.css": [
      "npm run lint:styles:fix",
      "git add"
    ],
    "src/**/*.html": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "express": "4.18.2",
    "nanoid": "4.0.0"
  }
}
